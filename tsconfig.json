/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "paths": {
      "@ng-camunda/core": [
        "dist/ng-camunda/core/ng-camunda-core",
        "dist/ng-camunda/core"
      ],
      "@ng-camunda/core/*": [
        "projects/ng-camunda/core/*",
        "projects/ng-camunda/core"
      ],
      "@demo-app/core/*":["projects/demo-app/src/app/core/*"],
      "@demo-app/*":["projects/demo-app/src/app/*"],
      "@demo-app/env/*":["projects/demo-app/src/environments/*"]
    },
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false
  }
}
